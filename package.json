{
    "name": "vue-boilerplate",
    "version": "0.0.1",
    "description": "A Vue-Webpack Boilerplate",
    "main": "main.js",
    "scripts": {
        "dev": "webpack-dev-server --hot --config config/webpack.dev.conf.js",
        "clean": "rm -rf ./public",
        "build:dll": "webpack --config config/webpack.dll.config.js",
        "prod": "webpack --config config/webpack.prod.conf.js",
        "build": "npm run clean && npm run build:dll && npm run prod"
    },
    "dependencies": {
        "axios": "^0.18.0",
        "vue": "^2.5.17",
        "vue-router": "^3.0.2"
    },
    "devDependencies": {
        "@babel/core": "^7.1.6",
        "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
        "@babel/plugin-syntax-dynamic-import": "^7.0.0",
        "@babel/plugin-transform-runtime": "^7.1.0",
        "@babel/polyfill": "^7.0.0",
        "@babel/preset-env": "^7.1.6",
        "add-asset-html-webpack-plugin": "^3.1.2",
        "autoprefixer": "^9.3.1",
        "babel-loader": "^8.0.4",
        "css-loader": "^1.0.1",
        "cssnano": "^4.1.7",
        "current-env": "^1.0.3",
        "eslint": "^5.9.0",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-plugin-html": "^4.0.5",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-jsx-a11y": "^6.1.2",
        "eslint-plugin-react": "^7.11.1",
        "eslint-plugin-vue": "^4.7.1",
        "happypack": "^5.0.0",
        "html-webpack-plugin": "^3.2.0",
        "mini-css-extract-plugin": "^0.4.5",
        "node-sass": "^4.10.0",
        "nyan-progress-webpack-plugin": "^1.2.0",
        "postcss-loader": "^3.0.0",
        "progress-bar-webpack-plugin": "^1.11.0",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "vue-loader": "^15.4.2",
        "vue-template-compiler": "^2.5.17",
        "webpack": "^4.26.1",
        "webpack-bundle-analyzer": "^3.0.3",
        "webpack-cli": "^3.1.2",
        "webpack-dashboard": "^2.0.0",
        "webpack-dev-server": "^3.1.14",
        "webpack-merge": "^4.1.4"
    }
}
